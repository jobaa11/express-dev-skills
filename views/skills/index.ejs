<%- include('../partials/header') %>

  <body>
    <h1>Todos</h1>
    <ul>
      <% todos.forEach(function(t) { %>
        <li>
          <a href="/todos/<%= t.id %>"><%= t.skill %></a>
            - 
          <%= t.done ? 'done' : 'not done' %>
        </li>
      <% }); %>
    </ul>
  </body>

</html>
